{
  "name": "tg-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.13.1",
    "immutable": "^3.8.2",
    "material-ui": "^1.0.0-beta.42",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "react-swipeable-views": "^0.12.13",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom --coverage --watch",
    "eject": "react-scripts eject",
    "codecov": "codecov -t $CODECOV_TOKEN",
    "deploy": "now --public -e NODE_ENV=production --npm && now alias",
    "deploy:ci": "now --public -e NODE_ENV=production --token $NOW_TOKEN --npm",
    "deploy:alias": "now alias --token=$NOW_TOKEN && now scale tg-list.now.sh 1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "devDependencies": {
    "codecov": "^3.0.0"
  }
}
