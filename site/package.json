{
  "name": "tg-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.13.1",
    "immutable": "^3.8.2",
    "material-ui": "^1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-swipeable-views": "^0.12.13",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "serve": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve -s ./build",
    "eslint": "eslint --ext .jsx src",
    "build": "react-scripts build",
    "build:prod": "NODE_ENV=prod react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "yarn test --watch",
    "eject": "react-scripts eject",
    "codecov": "codecov -t $CODECOV_TOKEN",
    "deploy": "now --public -e NODE_ENV=production --npm && now alias",
    "deploy:ci": "now --public -e NODE_ENV=production --token $NOW_TOKEN --npm",
    "deploy:alias": "now alias --token=$NOW_TOKEN && now scale --token=$NOW_TOKEN tg-list.now.sh all 1 1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{(js|jsx)}",
      "!src/index.js",
      "!src/api/firebase/*",
      "!src/registerServiceWorker.js"
    ]
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
